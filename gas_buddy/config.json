{
  "name": "Gas Buddy",
  "version": "0.0.1",
  "slug": "gas_buddy",
  "description": "Uses gasbuddy.com to get the local gas prices for the area specified",
  "url": "https://github.com/jackrazors/hassio-addons/",
  "startup": "application",
  "boot": "auto",
  "arch": ["amd64"],
  "map": ["share"],
  "ports": {
    "8080/tcp": 8080,
  },
  "webui": "http://[HOST]:[PORT:8080]",
  "options": {
    "config": "gasbuddy.json",
  },
  "schema": {
    "config": "str"
  },
  "image": "jackrazors/ha-gas-buddy"
}
